    "cucumber-prod": "NODE_ENV=prod ./node_modules/.bin/cucumber-js acceptance-tests/features/*.feature --tags @regression -r acceptance-tests/steps"
  },
  "author": "",
  "license": "Apache-2.0",
  "standard": {
